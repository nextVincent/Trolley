{
    "root": true,
    "env": {
        "node": true,
        "jest": true,
        "es6": true,
        "commonjs": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended",
      "prettier"
    ],
    "rules": {
        "semi": "error",
        "no-console": "error"
    }
  }